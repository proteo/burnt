      "paintBorder": true,
      "borderColor": "borderPrimary",
      "inactiveBorderColor": "borderPrimary",
      "Toolbar.borderColor": "borderPrimary",

      "Header": {
        "activeBackground": "backgroundTintFaint",
        "inactiveBackground": "backgroundTintFaint"
      },

      "Advertiser": {
        "foreground": "foregroundSecondary",
        "borderInsets": "4,8,4,0"
      }
    },

    "PopupMenu": {
      "background": "backgroundPrimary",
      "translucentBackground": "backgroundPrimary",
      "borderWidth": 1,
      "borderInsets": "7,1,7,1"
    },

    "PopupMenuSeparator": {
      "height": 14,
      "stripeIndent": 7,
      "stripeWidth": 1
    },

    "List": {
      "border": "0,0,0,0",
      "background": "backgroundPrimary",
      "noFocusBorder": "0,0,0,0",
      "rowHeight": 24
    },

    "ProgressBar": {
      "trackColor": "backgroundTintFaint",
      "progressColor": "#46468e",
      "indeterminateStartColor": "#46468e",
      "indeterminateEndColor": "#6b6ba5",
      "failedColor": "#5e0c0c",
      "failedEndColor": "#7e3d3d",
      "passedColor": "#135120",
      "passedEndColor": "#42744d"
    },

