{
  "name": "Burnt",
  "dark": true,
  "author": "Proteo",
  "editorScheme": "/themes/burnt.xml",

  "colors": {
    "white": "#ffffff",
    "black": "#000000",
    "foregroundPrimary": "#888b92",
    "foregroundSecondary": "#505058",
    "foregroundTintStrong": "#616166",
    "selectedForeground": "#fbfbfc",
    "backgroundPrimary": "#1d1d1d",
    "backgroundTintFaint": "#242424",
    "backgroundTintSoft": "#2e2b33",
    "backgroundTintStrong": "#46464a",
    "backgroundShadowSoft": "#161616",
    "borderPrimary": "#2b2b2b",
    "borderSecondary": "#161616",
