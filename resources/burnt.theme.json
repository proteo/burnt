      "focusedBorderColor": "#313133",
      "disabledText": "disabledText",
      "disabledBorderColor": "borderPrimary",

      "default": {
        "foreground": "selectedForeground",
        "focusColor": "focusPrimary",
        "startBackground": "#413d44",
