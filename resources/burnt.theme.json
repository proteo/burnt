      "background": "backgroundPrimary",
      "SidePanel.background": "272728",
      "separatorColor": "borderPrimary"
    },

    "OptionPane": {
      "messageForeground": "foregroundPrimary"
    },

    "window": "backgroundPrimary",
    "windowBorder": "borderSecondary",
    "windowText": "foregroundPrimary",

    "FileColor": {
