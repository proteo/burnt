      "background": "backgroundPrimary",
      "borderColor": "borderPrimary",

      "textForeground": "foregroundPrimary",
      "textBackground": "backgroundPrimary",
      "caretForeground": "foregroundPrimary",
      "infoForeground": "foregroundSecondary",

      "selectedForeground": "selectedForeground",
      "selectionForeground": "selectedForeground",
      "selectionBackground": "backgroundTintSoft",
      "selectionInactiveForeground": "selectedForeground",
      "selectionInactiveBackground": "#282629",
      "selectionForegroundInactive": "selectedForeground",
      "selectionBackgroundInactive": "#282629",
      "selectionEnabledColor": "selectedForeground",
      "selectionEnabledShadowColor": "selectedForeground",
      "selectionDisabledColor": "disabledText",
      "selectionDisabledShadowColor": "disabledText",

      "lightSelectionForeground": "foregroundPrimary",
      "lightSelectionBackground": "backgroundTintSoft",
      "lightSelectionInactiveForeground":"foregroundPrimary",
      "lightSelectionInactiveBackground": "#282629",

      "inactiveForeground": "foregroundTintStrong",
      "inactiveBackground": "#2d2a2f",

      "disabledText": "#545450",
      "disabledForeground": "disabledText",
      "disabledBackground": "borderPrimary",
      "disabledBorderColor": "borderPrimary",

      "focusColor": "focusPrimary",
      "focusedBorderColor": "focusPrimary",
      "focusWidth": 1,

      "errorForeground": "#f92672",

      "acceleratorForeground": "foregroundSecondary",
