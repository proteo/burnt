        "startBorderColor": "#403e42",
        "endBorderColor": "#403e42",
        "focusedBorderColor": "#403e42"
      }
    },

    "ActionButton": {
      "hoverBackground": "backgroundPrimary",
      "hoverBorderColor": "foregroundPrimary",
      "pressedBackground": "#393941",
      "pressedBorderColor": "#393941"
    },

    "ToggleButton": {
      "onForeground": "selectedForeground",
      "onBackground": "#2d2a2f",
      "offForeground": "#9f9fa6",
      "offBackground": "#2d2a2f",
      "buttonColor": "#403e42",
      "borderColor": "borderPrimary"
    },

    "DefaultTabs": {
      "hoverBackground": "borderPrimary",
      "underlineColor": "accentPrimary",
      "underlineHeight": 1,
      "inactiveUnderlineColor": "borderPrimary",
      "underlinedTabForeground": "selectedForeground",
      "underlinedTabBackground": "borderPrimary",
      "borderColor": "borderPrimary"
    },

    "TabbedPane": {
      "focusColor": "backgroundTintStrong",
