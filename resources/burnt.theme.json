      "Objects.Pink": "#f98b9e",
      "Objects.Yellow": "#ffd866",
      "Objects.Green": "#a9dc76",
      "Objects.Blue": "#78dce8",
      "Objects.Purple": "#ab9df3",
      "Objects.BlackText": "#000000ff",
      "Objects.YellowDark": "#B3B42B",
      "Objects.GreenAndroid": "#78c257",

      "Checkbox.Background.Default.Dark": "#1d1d1d",
      "Checkbox.Border.Default.Dark": "#383838",
      "Checkbox.Foreground.Selected.Dark": "#888b92",
      "Checkbox.Border.Selected.Dark": "#383838",
